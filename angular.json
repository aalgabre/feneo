{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss",
      "spec": false
    },
    "@schematics/angular:class": {
      "spec": false
    },
    "@schematics/angular:directive": {
      "spec": false
    },
    "@schematics/angular:guard": {
      "spec": false
    },
    "@schematics/angular:module": {
      "spec": false
    },
    "@schematics/angular:pipe": {
      "spec": false
    },
    "@schematics/angular:service": {
      "spec": false
    }
  },
  "newProjectRoot": "projects",
  "projects": {
    "spa-feneo": {
      "root": "projects/spa-feneo/",
      "sourceRoot": "projects/spa-feneo/src",
      "projectType": "application",
      "prefix": "fen",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "targets": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/spa-feneo",
            "index": "projects/spa-feneo/src/index.html",
            "main": "projects/spa-feneo/src/main.ts",
            "polyfills": "projects/spa-feneo/src/polyfills.ts",
            "tsConfig": "projects/spa-feneo/tsconfig.app.json",
            "assets": [
              "projects/spa-feneo/src/favicon.ico",
              "projects/spa-feneo/src/assets"
            ],
            "styles": [
              "projects/spa-feneo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/spa-feneo/src/environments/environment.ts",
                  "with": "projects/spa-feneo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "spa-feneo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "spa-feneo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "spa-feneo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/spa-feneo/src/test.ts",
            "polyfills": "projects/spa-feneo/src/polyfills.ts",
            "tsConfig": "projects/spa-feneo/tsconfig.spec.json",
            "karmaConfig": "projects/spa-feneo/karma.conf.js",
            "styles": [
              "projects/spa-feneo/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/spa-feneo/src/favicon.ico",
              "projects/spa-feneo/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/spa-feneo/tsconfig.app.json",
              "projects/spa-feneo/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "lib-feneo": {
      "root": "projects/lib-feneo",
      "sourceRoot": "projects/lib-feneo/src",
      "projectType": "library",
      "prefix": "fen",
      "targets": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/lib-feneo/tsconfig.lib.json",
            "project": "projects/lib-feneo/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/lib-feneo/src/test.ts",
            "tsConfig": "projects/lib-feneo/tsconfig.spec.json",
            "karmaConfig": "projects/lib-feneo/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/lib-feneo/tsconfig.lib.json",
              "projects/lib-feneo/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "feneo"
}